            prefWidth="720.0">
         <VBox.margin>
            <Insets bottom="10.0"/>
         </VBox.margin>
         <ImageView fx:id="imageView" fitHeight="200.0" fitWidth="720.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
               <Insets bottom="16.0" top="16.0"/>
            </HBox.margin>
         </ImageView>
      </HBox>
      <HBox maxWidth="-Infinity" prefHeight="98.0" prefWidth="720.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
         <Label prefHeight="22.0" text="Choice " translateX="32.0">
            <font>
               <Font size="15.0" />
            </font>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Label>
         <TextArea fx:id="textArea" prefHeight="98.0" prefWidth="560.0" translateX="60.0" wrapText="true" />
      </HBox>
      <HBox layoutX="10.0" layoutY="10.0" maxWidth="-Infinity" prefWidth="720.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
         <Label prefHeight="22.0" text="Grade  " translateX="32.0">
            <font>
               <Font size="15.0" />
            </font>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Label>
         <ComboBox fx:id="gradeComboBox" prefWidth="150.0" promptText="None" translateX="60.0" />
         <Button fx:id="insertPictureButton" contentDisplay="RIGHT" mnemonicParsing="false" text="Insert Picture" translateX="367.0">
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Button>
      </HBox>
   </VBox>
</HBox>
