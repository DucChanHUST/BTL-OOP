<HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="720.0" prefHeight="180.0" prefWidth="720.0" style="-fx-background-color: #dcdde1;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.GUI32ChoiceController">

   <VBox maxWidth="720.0" prefWidth="760.0">
